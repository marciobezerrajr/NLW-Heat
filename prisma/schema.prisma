generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

// criando a tabela Users
model User {
  id String @id @default(uuid())
  name String
  github_id Int
  avatar_url String
  login String

  @@map("users")
}

